<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
	<PropertyGroup>
		<OutputType>Library</OutputType>
		<LangVersion>13</LangVersion>
		<RootNamespace>Rebus</RootNamespace>
		<AssemblyName>Rebus.Microsoft.Extensions.Configuration</AssemblyName>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<Authors>mookid8000</Authors>
		<PackageProjectUrl>https://rebus.fm/what-is-rebus/</PackageProjectUrl>
		<Copyright>Copyright Rebus FM ApS 2012</Copyright>
		<PackageTags>rebus microsoft extensions configuration</PackageTags>
		<PackageDescription>Microsoft.Extensions.Configuration-based endpoint mappings configuration for Rebus</PackageDescription>
		<RepositoryUrl>https://github.com/rebus-org/Rebus.Microsoft.Extensions.Configuration</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageIcon>little_rebusbus2_copy-500x500.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\artwork\little_rebusbus2_copy-500x500.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="rebus" Version="8.6.1" />
		<PackageReference Include="microsoft.extensions.configuration" Version="[6.0.0, 11)" />
		<PackageReference Include="microsoft.extensions.configuration.binder" Version="[6.0.0, 11)" />
	</ItemGroup>
</Project>